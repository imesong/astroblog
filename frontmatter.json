{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/content/blog"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": "'blog'",
      "filePrefix": null,
      "clearEmpty": true,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "author",
          "name": "author",
          "type": "string",
          "single": true
        },
        {
          "title": "pubDatetime",
          "name": "pubDatetime",
          "type": "datetime",
          "default": "{{now}}"
        },
        {
          "title": "modDatetime",
          "name": "modDatetime",
          "type": "datetime"
        },
        {
          "title": "featured",
          "name": "featured",
          "type": "boolean"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "heroImage",
          "name": "heroImage",
          "type": "image",
          "isPreviewImage": true
        }
      ]
    }
  ],
  "frontMatter.dashboard.openOnStart": true
}